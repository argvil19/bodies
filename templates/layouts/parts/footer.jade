footer.page-footer.text-center
	.container
		.row
			div.col.l4.m4.s12
				section.navigation-nav
					h5.white-text Navigation
					ul
						li.bot10: a.grey-text.text-lighten-3(href='/') Home
						li.bot10: a.grey-text.text-lighten-3(href='/store/get') Programs
						li.bot10: a.grey-text.text-lighten-3(href='/contact') Contact
			div.col.l4.m4.s12
				section.social-nav.text-center
					h5.white-text Follow me
					ul
						li
							a(href=settings.facebookUrl)
								i.fa.fa-facebook
						li
							a(href=settings.twitterUrl)
								i.fa.fa-twitter
						li
							a(href=settings.instagramUrl)
								i.fa.fa-instagram
			div.col.l4.m4.s12
				section.content-nav
					h5.white-text More information
					if settings.phoneShow
						p.grey-text.text-lighten-4 Phone: #{settings.phone}
					p.grey-text.text-lighten-4 Email: #{settings.email}
					if settings.addressShow
						p.grey-text.text-lighten-4 Address: #{settings.address}

	.footer-copyright
		.container.text-center
			.dev
